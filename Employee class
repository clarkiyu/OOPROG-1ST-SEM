package employee;

public class Employee {
	
	String name;
	float salary;
	
	
	
	Employee (){
		this.name = "Not Assigned";
		this.salary = 0;
	}
	
	Employee(String name){
		this.name = name;
		this.salary = 0;
		
	}
	
	Employee(String name, float salary){
		this.name = name;
		this.salary = salary;
	}

}
